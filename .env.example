# GraphRAG API Keys and Configuration

# Database connections (Neo4j) - 
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=
NEO4J_PASSWORD=

# Qdrant connection (Qdrant)
QDRANT_HOST=
QDRANT_PORT=

# Collection settings (Qdrant)
COLLECTION_NAME=

# Model Provider - Choose one: "openai" or "ollama"
MODEL_PROVIDER=

# Vector settings
# For OpenAI: use 1536, For Ollama/nomic-embed-text: use 768
VECTOR_DIMENSION=

# Embedding model configuration
# Options: "text-embedding-3-small" (requires OpenAI) or "nomic-embed-text" (requires Ollama)
EMBEDDING_MODEL=

# LLM configuration for inference/completion
# Options: "gpt-4o-mini" (requires OpenAI) or "qwen2.5:3b" (requires Ollama)
LLM_MODEL=

# API connection settings
# For OpenAI
OPENAI_API_KEY=

# For Ollama
OLLAMA_HOST=
OLLAMA_PORT=

# Performance optimization parameters
PARALLEL_PROCESSING=
MAX_WORKERS=
BATCH_SIZE=
CHUNK_SIZE=